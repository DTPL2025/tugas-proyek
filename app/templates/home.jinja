{% extends "base.jinja" %}
{% block title %}Beranda{% endblock %}
{% block body %}
<nav>
    {% if username %}
        <h1>Selamat datang, {{ username }}!</h1>
        <a href="{{ url_for('logout') }}">Logout</a>
    {% else %}
        <a href="{{ url_for('register') }}">Daftar</a>
        <a href="{{ url_for('login') }}">Login</a>
    {% endif %}
</nav>
{% endblock %}
